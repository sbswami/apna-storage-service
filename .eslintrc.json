{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "semi": "error",
    // "eqeqeq": "warn",
    "quotes": ["warn", "single"],
    // "comma-dangle": "off",
    "no-console": "warn",
    // "indent": ["error", 2],
    // "linebreak-style": ["error", "unix"],
    // "no-cond-assign": ["error", "always"],
    "strict": "warn",
    // "no-empty":"warn",
    "max-len": ["error", {"code": 110}],
    "max-nested-callbacks": ["error", 3],
    "camelcase": "error"
  }
}
